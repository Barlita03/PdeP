class Modo {
    const multiplicadorFisico
    const multiplicadorMagico

    method multiplicadorFisico() = multiplicadorFisico
    method multiplicadorMagico() = multiplicadorMagico
}

const tranquilo = new Modo ( multiplicadorFisico = 1,  multiplicadorMagico = 1 )
const valiente = new Modo ( multiplicadorFisico = 1.5,  multiplicadorMagico = 0.8 )
const sabio = new Modo ( multiplicadorFisico = 3,  multiplicadorMagico = 0.2 )